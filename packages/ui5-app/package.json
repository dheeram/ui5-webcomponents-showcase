{
  "name": "ui5-webcomponents-app",
  "version": "0.0.1",
  "description": "UI5 application show-casing the usage of UI5 Web Components with UI5",
  "private": true,
  "scripts": {
    "start": "ui5 serve --config ui5.yaml --open index.html"
  },
  "dependencies": {
    "@openui5/sap.ui.core": "*",
    "@openui5/sap.ui.layout": "*",
    "@openui5/sap.m": "*",
    "@openui5/themelib_sap_fiori_3": "*"
  },
  "devDependencies": {
    "@ui5/cli": "*",
    "ui5-middleware-livereload": "*",
    "ui5-middleware-simpleproxy": "*"
  },
  "ui5": {
    "dependencies": [
      "@openui5/sap.ui.core",
      "@openui5/sap.m",
      "@openui5/sap.ui.layout",
      "@openui5/themelib_sap_fiori_3",
      "ui5-middleware-livereload",
      "ui5-middleware-simpleproxy"
    ]
  }
}
